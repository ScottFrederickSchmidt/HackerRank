'''
Mutations:
https://www.hackerrank.com/challenges/python-mutations/problem?isFullScreen=true
'''
def mutate_string(string, position, character):
    l=list(string)
    l[position]=character
    mutate=''.join(l)
    return mutate
---------------------

if __name__ == '__main__':
    n = int(input())
    final=''
    for i in range(n+1):
        final=final+str(i)
    print( int(final) )
